                 let maxDateString = `${date.getFullYear()}-${(date.getMonth()+1)<10?`0${(date.getMonth()+1)}`:(date.getMonth()+1)}-30`;
              %>
              <input type="date" class="calender-input" id="checkinDate" name="checkin" min=<%=mincheckinDateString%> max=<%= maxDateString %>>
            </div>
            <div class="col-2 field inpdiv">
